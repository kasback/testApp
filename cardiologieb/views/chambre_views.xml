<odoo>
  <data>

    <record model="ir.ui.view" id="cardiologieb.chambre">
      <field name="name">Chambres</field>
      <field name="model">cardiologieb.chambre</field>
      <field name="arch" type="xml">
        <tree>
          <field name="nom"/>
          <field name="secteur_id"/>
          <field name="dms"/>
          <field name="tmo"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="cardiologieb.chambre_form">
      <field name="name">Chambres</field>
      <field name="model">cardiologieb.chambre</field>
      <field name="arch" type="xml">
        <form>
          <group>
            <field name="nom"/>
            <field name="secteur_id"/>
            <field name="lits"/>
          </group>
        </form>
      </field>
    </record>

    <record model="ir.actions.act_window" id="cardiologieb.action_chambre">
      <field name="name">Chambres</field>
      <field name="res_model">cardiologieb.chambre</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>